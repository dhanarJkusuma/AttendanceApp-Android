<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dhanarjkusuma.sarihusada.sarihusada">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_sarihusada"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity" />
        <activity
            android:name=".view.LoginActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter
                android:label="@string/app_name">
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.DashboardRepsActivity" />
        <activity android:name=".view.DashboardSHActivity" />
        <activity android:name=".view.DashboardMyproActivity" />
        <activity
            android:name=".view.reps.RepsTambahPeserta"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".view.reps.RepsLihatPeserta" />
        <activity android:name=".view.reps.RepsUbahPeserta" />
        <activity android:name=".view.reps.RepsKloterPeserta" />
        <activity android:name=".view.sh.SHSearchBy" />
        <activity android:name=".view.sh.SHReadPeserta" />
        <activity android:name=".view.reps.RepsDetailPeserta" />
        <activity android:name=".view.sh.SHDetailPeserta" />
        <activity android:name=".view.mypro.MyProTambahPeserta" />
        <activity android:name=".view.mypro.MyProCategoryPeserta" />
        <activity android:name=".view.mypro.MyProLihatPeserta" />
        <activity android:name=".view.mypro.MyProUbahPeserta" />
        <activity android:name=".view.mypro.MyProDetailPeserta"></activity>
    </application>

</manifest>